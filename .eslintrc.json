{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": ["simple-import-sort"],
  "extends": ["next", "prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "complexity": ["error", 10]
  }
}
